<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <vType id="car" accel="1.0" decel="4.5" sigma="0.5" length="5" maxSpeed="13.9"/>

    <!-- Main corridor routes -->
    <route id="west_to_east" edges="west_in A_to_B B_to_east2"/>
    
    <!-- Side route variations -->
    <route id="northA_to_east" edges="northA_in A_to_B B_to_east2"/>
    <route id="southA_to_east" edges="southA_in A_to_B B_to_east2"/>
    <route id="west_to_northB" edges="west_in A_to_B B_to_northB"/>
    <route id="west_to_southB" edges="west_in A_to_B B_to_southB"/>

    <!-- Flows -->
    <flow id="flow1" type="car" route="west_to_east" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="flow2" type="car" route="northA_to_east" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="flow3" type="car" route="southA_to_east" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="flow4" type="car" route="west_to_northB" begin="0" end="3600" vehsPerHour="150"/>
    <flow id="flow5" type="car" route="west_to_southB" begin="0" end="3600" vehsPerHour="150"/>

</routes>