<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" maxSpeed="13.9"/>

    <!-- Routes -->
    <route id="route1" edges="west_in A_to_B B_to_east"/>
    <route id="route2" edges="northA_in A_to_B B_to_east"/>
    <route id="route3" edges="southA_in A_to_B B_to_east"/>
    <route id="route4" edges="west_in A_to_B B_to_northB"/>
    <route id="route5" edges="west_in A_to_B B_to_southB"/>

    <!-- Vehicle flows -->
    <flow id="flow1" type="car" route="route1" begin="0" end="3600" vehsPerHour="300"/>
    <flow id="flow2" type="car" route="route2" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="flow3" type="car" route="route3" begin="0" end="3600" vehsPerHour="200"/>
    <flow id="flow4" type="car" route="route4" begin="0" end="3600" vehsPerHour="150"/>
    <flow id="flow5" type="car" route="route5" begin="0" end="3600" vehsPerHour="150"/>

</routes>